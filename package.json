{
  "name": "feather",
  "version": "1.0.0",
  "description": "Theme for Shopify 2.0",
  "main": "./scripts/index.js",
  "scripts": {
    "test": "cypress open",
    "sass": "sass --watch ./styles/_style.scss ./assets/style.css",
    "js": "rollup scripts/index.js --file ./assets/script.js --format iife --watch",
    "dev": "concurrently --kill-others \"sass --watch ./styles/style.scss ./assets/style.css\" \"rollup scripts/index.js --file ./assets/script.js --format iife --watch\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NicholasEli/shopify-feather-theme.git"
  },
  "author": "NicholasByDesign",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NicholasEli/shopify-feather-theme/issues"
  },
  "homepage": "https://github.com/NicholasEli/shopify-feather-theme#readme",
  "devDependencies": {
    "@rollup/plugin-json": "4.1.0",
    "concurrently": "7.2.0",
    "cypress": "9.5.4",
    "cypress-real-events": "1.7.0"
  }
}
