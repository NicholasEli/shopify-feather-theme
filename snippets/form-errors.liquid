<script>
	window.Feather.form_errors.push({ field: 'email', message: 'Email address is invalid' });
	{% for error in errors %}
		{% if form == 'contact' %}
			{% if error == 'email' %}
				window.Feather.form_errors.push({ field: 'email', message: 'Email address is invalid' });
			{% endif %}
		{% endif %}
	{% endfor %}
</script>